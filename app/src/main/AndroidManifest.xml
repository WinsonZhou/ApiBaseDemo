            android:screenOrientation="landscape" />
        <activity
            android:name=".view.PBActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.MeasureViewActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="b553012948bed2a802eaa62344bb2bae" />
        <meta-data
            android:name="winson"
            android:value="${winson}" />
