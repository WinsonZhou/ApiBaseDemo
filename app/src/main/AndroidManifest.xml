        <activity android:name=".animation.DrawableAnimationActivity" />
        <activity android:name=".view.EmptyListViewActivity" />
        <activity android:name=".performance.PerformanceListActivity" />
        <activity android:name=".performance.MemoryActivity" />
        <activity android:name=".performance.MessageActivity" />
        <activity android:name=".performance.MemoryLeakActivity" />
        <activity android:name=".view.RecyclerViewActivity" />
        <activity android:name=".view.MultipleViewTypeListViewActivity" />
        <activity android:name=".drawable.VectorDrawableActivity" />
        <activity android:name=".drawable.DrawableListActivity" />
        <activity android:name=".sdk.SdkListActivity" />
        <activity android:name=".sdk.QQSdkActivity" />
        <activity android:name=".sdk.WeiboSdkActivity" />
        <activity android:name=".view.MyScrollerActivity" />
        <activity
            android:name=".sdk.WechatSdkActivity"
            android:exported="true" />
        <activity android:name=".csdn.CsdnListActivity" />
        <activity android:name=".csdn.circle_media.CircleNoBlackVideoActivity" />
        <activity android:name=".design.onceloadpager.OnceLoadPagerActivity" />
        <activity android:name=".design.multitypelistview.MultiTypeListViewActivity" />
        <activity android:name=".attr.AttrActivity" />
        <activity android:name=".attr.AttrListActivity" />
        <activity android:name=".design.emptyview.EmptyViewActivity" />
        <activity android:name=".design.emptyview.DialogFragmentEmptyActivity" />
        <activity android:name=".sdk.VolleySdkActivity" />
        <activity android:name=".csdn.PluginDemoActivity" />
        <activity android:name=".performance.HandlerActivity" />
        <activity android:name=".support.MyWebViewActivity" />
        <activity android:name=".view.SortGridActivity" />
        <activity android:name=".support.UpgradeActivity" />
        <activity android:name=".drawable.CustomDrawableActivity" />
        <activity android:name=".support.CrashLogActivity" />
        <activity android:name=".drawable.ShadowActivity" />
        <activity android:name=".support.LocationActivity" />
        <activity
